@model IEnumerable<BE_S6_L1.Models.Studente>

<div class="list-group">
    @foreach (var item in Model)
    {
        <a href="#" class="list-group-item list-group-item-action student-item" data-id="@item.Id">
            <div class="d-flex w-100 justify-content-between">
                <h5 class="mb-1">@item.Nome @item.Cognome</h5>
            </div>
            <p class="mb-1">@item.Email</p>
        </a>
    }
</div>